<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.jvnet.hudson.plugins</groupId>
    <artifactId>plugin</artifactId>
    <version>1.391</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <artifactId>selenium-aes</artifactId>
  <version>0.6-SNAPSHOT</version>
  <packaging>hpi</packaging>
  <name>Selenium Auto Exec Server(AES) plugin</name>
  <description>
    This plugin is a Selenium Auto Exec Server(AES) (http://www.enjoyxstudy.com/selenium/autoexec/) plugin.
  </description>
  <url>http://wiki.hudson-ci.org/display/HUDSON/Selenium+AES+Plugin</url>

  <developers>
    <developer>
      <id>onozaty</id>
    </developer>
  </developers>

  <dependencies>
    <dependency>
      <groupId>com.enjoyxstudy</groupId>
      <artifactId>selenium-autoexec-client</artifactId>
      <version>1.1</version>
      <scope>system</scope>
      <systemPath>${basedir}/src/main/webapp/WEB-INF/lib/selenium-autoexec-client-1.1.jar</systemPath>
    </dependency>
  </dependencies>

    <repositories>
        <repository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </pluginRepository>
    </pluginRepositories>
</project>  
  

